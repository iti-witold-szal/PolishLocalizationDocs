{
  "build": {
    "content": [
      {
        "files": [
          "PolishFunctionality/**.md",
          "PolishFunctionality/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["**media/**"],
        "exclude": [
        "**/obj/**",
        "**/includes/**"
        ]
      }
    ],
    "overwrite": [
      {
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "IT.integro",
      "_appFooter": "Copyright Â© IT.integro sp. z o.o.",
      "_disableContribution": true,
      "_enableSearch": "true"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "../ProductSiteTemplate/templates/itintegro"
      ],
    "postProcessors": ["ExtractSearchIndex"],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
